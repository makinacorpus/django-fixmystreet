<services>{% for service in services %}
    <service>
        <service_code>{{service.service_code}}</service_code>
        <service_name>{{service.service_name}}</service_name>
        <description>{{service.description}}</description>
        <metadata>{{service.metadata}}</metadata>
        <type>{{service.type}}</type>
        <keywords>{{service.keywords}}</keywords>
        <group>{{service.group}}</group>
    </service>{% endfor %}
</services>

